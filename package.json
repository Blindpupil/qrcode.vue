{
  "name": "qrcode.vue",
  "version": "0.3.0",
  "description": "A Vue component for QR_code.",
  "main": "dist/qrcode.vue.min.js",
  "scripts": {
    "start": "better-npm-run start",
    "clean": "rimraf -- dist",
    "compile": "better-npm-run compile",
    "github:page": "better-npm-run github:page",
    "deploy": "better-npm-run deploy",
    "deploy:min": "better-npm-run deploy:min",
    "prepublish": "npm run deploy:min"
  },
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --config webpack.config.js --open --progress --profile --watch --content-base src/",
      "env": {
        "EXAMPLE": "example",
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "node ./bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "github:page": {
      "command": "rimraf -- github-page && npm run compile",
      "env": {
        "EXAMPLE": "example",
        "MINIMIZE": "minimize",
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy:min": {
      "command": "npm run clean && npm run compile",
      "env": {
        "MINIMIZE": "minimize",
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scopewu/qrcode.vue.git"
  },
  "keywords": [
    "vue",
    "vue-component",
    "qrcode"
  ],
  "author": "scope.wu <scope.wu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scopewu/qrcode.vue/issues"
  },
  "homepage": "https://scopewu.github.io/qrcode.vue/",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "better-npm-run": "0.0.15",
    "debug": "^2.6.3",
    "html-webpack-plugin": "^2.28.0",
    "qr.js": "0.0.0",
    "rimraf": "^2.6.1",
    "vue": "^2.2.6",
    "vue-loader": "^11.3.4",
    "vue-template-compiler": "^2.2.6",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
